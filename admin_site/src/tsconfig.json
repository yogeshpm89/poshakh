{
  "compilerOptions": {
    "baseUrl": "",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es6", "dom"],
    "mapRoot": "./",
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "es5",
    "removeComments": true,
    "typeRoots": [
      "../node_modules/@types",
      "./node_modules"
    ],
    "rootDir": "./app",
    "outFile": "dist/bundle.js"
  }
}